[run]
branch = True
parallel = True
source = src/scripts

[report]
show_missing = True
skip_covered = True
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
omit =
    tests/*
    */__init__.py

[html]
directory = htmlcov
title = Coverage Report for jps-yaml-schema-validator
show_contexts = true
